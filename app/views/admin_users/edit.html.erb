<% @page_title = 'Edit Admin' %>

<div class="admin_users edit">
  <h2>Update Admin <%= @admin_user.full_name %></h2>
  <%= link_to('<< Back to index', admin_users_path, class: 'back-link') %>

  <%= form_for(@admin_user) do |f| %>
    <%= render(partial: 'form', locals: { f: f }) %>

    <div class="form-buttons">
      <%= f.submit('Update Admin') %>
    </div>
  <% end %>
</div>