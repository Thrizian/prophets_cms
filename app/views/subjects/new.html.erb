<%= render 'shared/title' %>

<div class="subjects new">
  <h2>Create a new Subject</h2>

  <%= link_to('<< Back to index', subjects_path, class: 'back-link') %>

  <%= form_for(@subject) do |f| %>
    <%= render(partial: 'form', locals: {f: f}) %>

    <div class="form-buttons">
      <%= f.submit('Create Subject') %>
    </div>
  <% end %>
</div>