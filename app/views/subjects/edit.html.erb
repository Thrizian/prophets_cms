<%= render 'shared/title' %>
<div class="subjects edit">
  <h2>Update Subject <%= @subject.name %></h2>

  <%= render partial: 'back_to_index' %>

  <%= form_for(@subject) do |f| %>
    <%= render(partial: 'form', locals: {f: f}) %>

    <div class="form-buttons">
      <%= f.submit('Update Subject') %>
    </div>
  <% end %>
</div>